{
    "name": "ellipse/cache",
    "type": "library",
    "description": "the cache component of the ellipse framework",
    "keywords": [
        "psr-6",
        "cache",
        "redis",
        "predis",
        "encryption"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/cache",
    "support": {
        "issues": "https://github.com/ellipsephp/cache/issues",
        "source": "https://github.com/ellipsephp/cache"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/cache": "^1.0",
        "cache/taggable-cache": "^0.5-dev",
        "cache/predis-adapter": "^0.5-dev",
        "cache/filesystem-adapter": "^0.4-dev",
        "cache/encryption-cache": "^0.2-dev",
        "predis/predis": "^1.1",
        "defuse/php-encryption": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Cache\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
