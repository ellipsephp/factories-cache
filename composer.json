{
    "name": "ellipse/cache",
    "type": "library",
    "description": "The cache component of the ellipse framework",
    "keywords": [
        "psr-6",
        "cache",
        "factories",
        "filesystem",
        "flysystem",
        "redis",
        "predis",
        "encryption"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/cache",
    "support": {
        "issues": "https://github.com/ellipsephp/cache/issues",
        "source": "https://github.com/ellipsephp/cache"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "container-interop/service-provider": "^0.3.0",
        "psr/cache": "^1.0",
        "ellipse/binder": "^1.0",
        "cache/filesystem-adapter": "^1.0",
        "cache/predis-adapter": "^1.0",
        "cache/taggable-cache": "^1.0",
        "cache/encryption-cache": "^1.0"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "^1.6",
        "mockery/mockery": "^1.0-alpha",
        "league/flysystem": "^1.0",
        "predis/predis": "^1.1",
        "defuse/php-encryption": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Cache\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "bindings": {
            "classes": {
                "service-providers": [
                    "Ellipse\\Cache\\CacheServiceProvider"
                ]
            }
        }
    }
}
